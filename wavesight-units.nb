Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"EMUnits", "::", "usage"}], "=", 
   "\"\<EMUnits[{unitASymbol,unitAInSI},{unitASymbol,unitAInSI}] determines \
the expressions for the units of different electro-mechanical units onece the \
choice of \[Epsilon]0=\[Mu]0=1 has been taken. unitASymbol is a symbol that \
would be used to represent a unit, and unitAInSI would be the correspondence \
that this unit has in MKSA system. Similarly for unitB.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EMUnits", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a_", ",", "A_"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"b_", ",", "B_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"unitTable", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"(", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"\[Epsilon]0", "=", 
          RowBox[{
           SuperscriptBox["C", "2"], 
           FractionBox[
            SuperscriptBox["s", "2"], 
            RowBox[{"kg", " ", 
             SuperscriptBox["m", "3"]}]]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"\[Mu]0", "=", 
          RowBox[{"kg", " ", 
           FractionBox["m", 
            SuperscriptBox["C", "2"]]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"baseUnits", "=", 
          RowBox[{"{", 
           RowBox[{"C", ",", "s", ",", "kg", ",", "m"}], "}"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"lhs", "=", 
          RowBox[{
           SuperscriptBox["\[Epsilon]0", "n1"], 
           SuperscriptBox["\[Mu]0", "n2"], 
           SuperscriptBox["A", "n3"], 
           SuperscriptBox[
            RowBox[{"(", "B", ")"}], "n4"]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"rhs", "=", 
          RowBox[{"quants", "[", "quant", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"eqns", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Exponent", "[", 
              RowBox[{"lhs", ",", "unit"}], "]"}], "==", 
             RowBox[{"Exponent", "[", 
              RowBox[{"rhs", ",", "unit"}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"unit", ",", "baseUnits"}], "}"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"sol", "=", 
          RowBox[{"Solve", "[", 
           RowBox[{"eqns", ",", 
            RowBox[{"{", 
             RowBox[{"n1", ",", "n2", ",", "n3", ",", "n4"}], "}"}]}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"ClearAll", "[", 
          RowBox[{"\[Epsilon]0", ",", "\[Mu]0"}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"uSol", "=", 
          RowBox[{
           RowBox[{
            SuperscriptBox["\[Epsilon]0", "n1"], 
            SuperscriptBox["\[Mu]0", "n2"], 
            SuperscriptBox["a", "n3"], 
            SuperscriptBox["b", "n4"]}], "/.", 
           RowBox[{"sol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"uSol", "=", 
          RowBox[{"uSol", "/.", 
           RowBox[{"{", 
            RowBox[{"\[Mu]0", "->", 
             FractionBox["1", 
              RowBox[{
               SuperscriptBox["c", "2"], "\[Epsilon]0"}]]}], "}"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"uSol", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{"PowerExpand", "[", "uSol", "]"}], ",", 
            RowBox[{"Assumptions", "->", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"c", ">", "0"}], ",", 
               RowBox[{"\[Epsilon]0", ">", "0"}]}], "}"}]}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"quant", ",", "uSol"}], "}"}]}], ")"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"quant", ",", 
         RowBox[{"Keys", "[", "quants", "]"}]}], "}"}]}], 
      "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Return", "[", "unitTable", "]"}], ";"}], ")"}]}]}], "Input",
 InitializationCell->True,
